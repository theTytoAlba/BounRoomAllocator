<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thetytoalba.bounroomallocator"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="26" />

    <uses-permission android:name="android.permission.INTERNET" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="26.0.0-alpha1" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/boun_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/boun_logo"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.thetytoalba.bounroomallocator.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.thetytoalba.bounroomallocator.MainActivity" />
        <activity android:name="com.thetytoalba.bounroomallocator.SignUpActivity" >
        </activity>
    </application>

</manifest>